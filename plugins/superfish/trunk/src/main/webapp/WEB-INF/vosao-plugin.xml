<vosao-plugin>
    
    <name>superfish</name>
    
    <title>Menu superfish plugin</title>
    
    <description>
        Superfish plugin allows to create menu based on Superfish jQuery plugin.
    </description>
    
    <website>www.vosao.org</website>

    <velocity-plugin-class>
        org.vosao.plugins.superfish.SuperfishVelocityPlugin
    </velocity-plugin-class>

    <plugin-config-url>config.html</plugin-config-url>

    <back-service-manager-class>
        org.vosao.plugins.superfish.SuperfishBackServiceManager
    </back-service-manager-class>

    <header-javascript>js/hoverIntent.js</header-javascript>
    <header-javascript>js/superfish.js</header-javascript>
    <header-css>css/superfish.css</header-css>
    
</vosao-plugin>